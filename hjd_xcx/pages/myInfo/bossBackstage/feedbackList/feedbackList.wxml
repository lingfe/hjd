<view class='page'>
  <view class='page_view'>
    <view class='view_model' wx:for="{{list}}" wx:key="*this">
      <view class='model_content'>{{item.content}}</view>
      <view class='model_btn'>
        <view class='btn_update' bindtap='btn_update' id="{{item.content}}">查看</view>
        <view class='btn_delete' bindtap='btn_delete' id="{{item.id}}">删除</view>
      </view>
    </view>
  </view>
</view>
